<div class="container-home">
  <div class="row justify-content-center">
    <div class="col-4">
      <div class="sidebar bg-warnig">
        <div class="skill_barre">
          <span>html</span>
            <div class="progress" role="progressbar" aria-label="Animated striped example" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100">
             <div class="progress-bar progress-bar-striped progress-bar-animated" style="width: 90%">90%</div>
            </div>
           <span>css</span>
           <div class="progress" role="progressbar" aria-label="Basic example" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">
              <div class="progress-bar progress-bar-striped progress-bar-animated" style="width: 85%">85%</div>
            </div>
           <span>js</span>
           <div class="progress" role="progressbar" aria-label="Basic example" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">
              <div class="progress-bar progress-bar-striped progress-bar-animated" style="width: 70%">70%</div>
            </div>
           <span>java</span>
           <div class="progress" role="progressbar" aria-label="Basic example" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100">
              <div class="progress-bar progress-bar-striped progress-bar-animated" style="width: 60%">60%</div>
            </div>
           <span>php</span>
           <div class="progress" role="progressbar" aria-label="Basic example" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100">
              <div class="progress-bar progress-bar-striped progress-bar-animated" style="width: 80%">80%</div>
            </div>
          </div>
          <div class="skill_loading">
          
        </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="profil bg-primary"></div>
      </div>
      <div class="row">
        <div class="col">
          <div class="experiences bg-primary">
          <% if false%>
             <%= simple_form_for [@user, @experience] do |f| %>
              <%= f.association :company %>
              <%= f.input :job_name, label: "Job Name" %>
              <%= f.input :job_description, label: "Job Description" %>
              <%= f.input :start_date, as: :date, label: "Start Date" %>
              <%= f.input :end_date, as: :date, label: "End Date" %>
              <%= f.input :skills, label: "Skills" %>

              <%= f.button :submit, class: "btn btn-primary" %>
              <% end %>
            <% end %>
              <%# <%= f.button :submit, value: "Update", class: "btn btn-primary" %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-4">
      <div class="languages">
      </div>
    </div>
    <div class="col_4">
      <div class="companies">
      </div>
    </div>
    <div class="col-4">
      <div class="share">
      </div>
    </div>
  </div>
</div>
